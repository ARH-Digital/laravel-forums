{
    "name": "bishopb/vanilla-for-laravel",
    "description": "Laravel 4 package of Vanilla Forums",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Bishop Bettini", "email": "bishop@php.net"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "https://github.com/vanilla/vanilla" }
    ],
    "require": {
        "laravel/framework": "~4.2",
        "vanilla/vanilla": "dev-master@dev",
        "watson/validating": "0.10.*",
        "felixkiss/uniquewith-validator": "1.1.*"
    },
    "autoload": {
        "psr-4": {
            "BishopB\\Vfl\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable"
}
